<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="tests.AnnotationTransformer"/>
        <listener class-name="tests.TestListener"/>
    </listeners>
    <test name="Cart">
        <classes>
            <class name="tests.ContextMenuTest"/>
        </classes>
    </test>
    <test name="CheckOut">
        <classes>
            <class name="tests.DynamicControlsTest"/>
        </classes>
    </test>
    <test name="Login">
        <classes>
            <class name="tests.FileUploaderTest"/>
        </classes>
    </test>
    <test name="Products">
        <classes>
            <class name="tests.FrameTest"/>
        </classes>
    </test>
</suite>
